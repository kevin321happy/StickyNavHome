<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wh.jxd.com.stickynavhome">
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS"/>

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.FLASHLIGHT" />

    <uses-feature android:name="android.hardware.camera" />
    <uses-feature android:name="android.hardware.camera.autofocus" />

    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:name=".AppcationEx"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <!--百分比布局-->

        <meta-data
            android:name="design_width"
            android:value="1080">
        </meta-data>
        <meta-data
            android:name="design_height"
            android:value="1920">
        </meta-data>

        <activity android:name=".ui.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.TestViewActivity"/>
        <activity android:name=".sample.ui.activtiy.CampaignActivity"/>
        <activity android:name=".sample.ui.activtiy.ExamActivity"/>
        <activity android:name=".sample.ui.activtiy.TrainingHelperActivity"/>
        <activity android:name=".sample.ui.activtiy.ToolSettingActivity"
            android:launchMode="singleTop"/>

        <activity android:name=".sample.ui.activtiy.CallCentryActivtiy"/>

        <activity android:name=".sample.ui.activtiy.SettingTwoCodeActivity"/>
    </application>

</manifest>